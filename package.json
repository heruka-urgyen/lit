{
  "name": "@heruka_urgyen/lit",
  "version": "0.0.0",
  "description": "interactive git tools",
  "main": "src/index.js",
  "scripts": {
    "add": "node dist/add.js",
    "build": "parcel build src/add.js --target node",
    "dev": "concurrently --raw 'parcel --log-level 0 watch src/add.js --target node' 'nodemon --watch src --exec npm run lint' 'npm run test'",
    "lint": "eslint src .eslintrc.js",
    "test": "ava --watch --verbose"
  },
  "keywords": [
    "interactive",
    "git",
    "tools",
    "react",
    "blessed"
  ],
  "author": "Ilya Malyanov",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "ava": "^3.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "esm": "^3.2.25",
    "ink-testing-library": "^2.0.1",
    "nodemon": "^2.0.4",
    "parcel": "^1.12.4",
    "test-console": "^1.1.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cli-cursor": "^3.1.0",
    "ink": "^3.0.3",
    "node-pty": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "strip-ansi": "^6.0.0"
  },
  "ava": {
    "require": [
      "esm",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
